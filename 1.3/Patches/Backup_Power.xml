<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<!-- ID: 2084493662 -->
	<Operation Class="PatchOperationFindMod">
		<mods><li>Backup Power</li></mods>
		<match Class="PatchOperationSequence">
			<operations>
				
				
				<li Class="PatchOperationRemove">
					<xpath>/Defs/ResearchProjectDef[defName="BackupPower"]</xpath>
				</li>
				
				<li Class="PatchOperationReplace">
					<xpath>/Defs/ThingDef[defName="BackupPower_Attachment"]/researchPrerequisites</xpath>
					<value>
						<researchPrerequisites>
							<li>Batteries</li>
						</researchPrerequisites>
					</value>
				</li>
				
			</operations>
		</match>
	</Operation>
</Patch>