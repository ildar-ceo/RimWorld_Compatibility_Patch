							<li>MeatRaw</li>
						</thingCategories>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[defName="RawFungus"]/ingestible</xpath>
					<value>
						<preferability>RawTasty</preferability>
