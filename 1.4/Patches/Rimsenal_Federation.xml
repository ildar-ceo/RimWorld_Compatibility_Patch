<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<!-- ID: 736172213 -->
	<Operation Class="PatchOperationFindMod">
		<mods><li>Rimsenal - Federation Faction Pack</li></mods>
		<match Class="PatchOperationSequence">
			<operations>
				
				<li Class="PatchOperationAdd">
					<xpath>/Defs/TraderKindDef[defName="Caravan_Outlander_Exotic" or defName="Orbital_BulkGoods"]/stockGenerators</xpath>
					<value>
						<li Class="StockGenerator_SingleDef">
							<thingDef>Foerum</thingDef>
							<countRange>10~50</countRange>
						</li>
						<li Class="StockGenerator_SingleDef">
							<thingDef>CrucibleCore</thingDef>
							<countRange>2~5</countRange>
						</li>
					</value>
				</li>
				
				<li Class="PatchOperationAdd">
					<xpath>/Defs/TraderKindDef[defName="Orbital_Exotic"]/stockGenerators</xpath>
					<value>
						<li Class="StockGenerator_SingleDef">
							<thingDef>Foerum</thingDef>
							<countRange>100~200</countRange>
						</li>
						<li Class="StockGenerator_SingleDef">
							<thingDef>CrucibleCore</thingDef>
							<countRange>10~50</countRange>
						</li>
					</value>
				</li>
				
			</operations>
		</match>
	</Operation>
	
	
	<!-- ID: 736172213 -->
	<Operation Class="PatchOperationFindMod">
		<mods><li>Rimsenal - Federation Faction Pack</li></mods>
		
		<!-- ID: 1854376306 -->
		<match Class="PatchOperationFindMod">
			<mods><li>Nyaron race</li></mods>
			<match Class="PatchOperationSequence">
				<operations>
					
					<li Class="PatchOperationAdd">
						<xpath>/Defs/TraderKindDef[defName="Caravan_Nyaron_Exotic"]/stockGenerators</xpath>
						<value>
							<li Class="StockGenerator_SingleDef">
								<thingDef>Foerum</thingDef>
								<countRange>10~50</countRange>
							</li>
							<li Class="StockGenerator_SingleDef">
								<thingDef>CrucibleCore</thingDef>
								<countRange>2~5</countRange>
							</li>
						</value>
					</li>
					
				</operations>
			</match>
		</match>
		
	</Operation>
	
	
</Patch> 
 
