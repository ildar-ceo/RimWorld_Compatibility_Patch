<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	
	<!-- ID: 2155485488 -->
	<Operation Class="PatchOperationFindMod">
		<mods><li>Cybernetic Warfare and Special Weapons (Continued)</li></mods>
		<match Class="PatchOperationSequence">
			<operations>
				
				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[defName="CW_NapalmBlaster" or defName="CW_Accelerator" or defName="CW_Disruptor" or defName="CW_DualEraser"]/tradeTags</xpath>
					<value>
						<li>ExoticMisc</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[defName="CW_GunBlade"]</xpath>
					<value>
						<tradeability>Sellable</tradeability>
					</value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/ThingDef[defName="CW_GunBlade"]/statBases/MarketValue</xpath>
					<value>
						<MarketValue>4000</MarketValue>
					</value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/ThingDef[defName="CW_GunBlade"]/statBases/RangedWeapon_Cooldown</xpath>
					<value>
						<RangedWeapon_Cooldown>0.3</RangedWeapon_Cooldown>
					</value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/ThingDef[defName="CW_GunBlade"]/costList</xpath>
					<value>
						<costList>
							<Gold>20</Gold>
							<Uranium>100</Uranium>
							<Plasteel>150</Plasteel>
							<ComponentSpacer>25</ComponentSpacer>
							<ComponentIndustrial>50</ComponentIndustrial>
						</costList>
					</value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/ResearchProjectDef[defName="CW_CyberneticWarfare"]/baseCost</xpath>
					<value>
						<baseCost>2400</baseCost>
					</value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/ResearchProjectDef[defName="CW_FrameworkWeaponry"]/baseCost</xpath>
					<value>
						<baseCost>7000</baseCost>
					</value>
				</li>
				
			</operations>
		</match>
	</Operation>
	
	
