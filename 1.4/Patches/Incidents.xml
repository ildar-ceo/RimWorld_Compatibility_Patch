<?xml version="1.0" encoding="utf-8" ?>

<Patch>
	
	<!-- ID: 1123043922 -->
	<Operation Class="PatchOperationFindMod">
		<mods><li>Sparkling Worlds - Full Mod</li></mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/IncidentDef[defName="InsectHiveDropSW"]/baseChance</xpath>
					<value>
						<baseChance>0.75</baseChance>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
	
	
	<!-- ID: 1938420742 -->
	<Operation Class="PatchOperationFindMod">
		<mods><li>Vanilla Events Expanded</li></mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/IncidentDef[defName="VEE_Drought" or defName="VEE_AnimalPodCrash" or defName="VEE_Earthquake" or defName="VEE_CaravanAnimalsWanderIn" or defName="VEE_BattleAnimalsWanderIn" or defName="VEE_WildMenWanderIn" or defName="VEE_Cargopodsweapons" or defName="VEE_Cargopodsapparel"]/baseChance</xpath>
					<value>
						<baseChance>0.01</baseChance>
					</value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/IncidentDef[defName="VEE_IceAge" or defName="VEE_GlobalWarming" or defName="VEE_PsychicRain" or defName="VEE_LongNight" or defName="VEE_PsychicBloom"]/baseChance</xpath>
					<value>
						<baseChance>0.25</baseChance>
					</value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/IncidentDef[defName="VEE_IceAge" or defName="VEE_GlobalWarming" or defName="VEE_PsychicRain" or defName="VEE_LongNight" or defName="VEE_PsychicBloom"]/minRefireDays</xpath>
					<value>
						<minRefireDays>150</minRefireDays>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
	
	
